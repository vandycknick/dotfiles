Host *
  IdentitiesOnly yes
  IdentityFile none
  ForwardAgent no
  ForwardX11 no
  ClearAllForwardings yes
  StrictHostKeyChecking ask
  UserKnownHostsFile ~/.ssh/known_hosts
  CanonicalizeHostname yes
  VerifyHostKeyDNS yes

Host 192.168.10.*
  User admin
  IdentityFile ~/.ssh/id_rsa
  IdentitiesOnly no
  PreferredAuthentications publickey

Host github.com
  HostName github.com
  User git
  IdentitiesOnly no
  IdentityFile ~/.ssh/id_rsa

Host *.dev.azure.com
    User git
    IdentityFile ~/.ssh/id_rsa
    IdentitiesOnly yes
    PreferredAuthentications publickey
